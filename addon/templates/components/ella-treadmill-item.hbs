<style type="text/css">
  #{{this.elementId}} {
    position: relative;
    height: {{@height}}{{
    @heightUnit
  }};
    width: {{this.width}}{{
    this.widthUnit
  }};
    box-sizing: border-box;
    display: block;
    flex: 0 0 {{
    this.width
  }}{{this.widthUnit}};

    -o-transform: translateY({{
    this.translateY
  }});
    -o-transform: translate3d(0, {{
    this.translateY
  }}, 0);

    -ms-transform: translateY({{
    this.translateY
  }});
    -ms-transform: translate3d(0, {{
    this.translateY
  }}, 0);

    -moz-transform: translateY({{
    this.translateY
  }});
    -moz-transform: translate3d(0, {{
    this.translateY
  }}, 0);

    -webkit-transform: translateY({{
    this.translateY
  }});
    -webkit-transform: translate3d(0, {{
    this.translateY
  }}, 0);

    transform: translateY({{
    this.translateY
  }});
    transform: translate3d(0, {{this.translateY}}, 0);
  }
</style>

{{yield @item @index}}